FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          ( water air );

transportModel  Newtonian;

nu              nu [ 0 2 -1 0 0 0 0 ] 1e-06;

rho             rho [ 1 -3 0 0 0 0 0 ] 1000;

sigma           sigma [ 1 0 -2 0 0 0 0 ] 0;


